{
  "name": "github-pr-experimental",
  "version": "0.0.12",
  "description": "test",
  "scripts": {
    "commit": "npx git-cz",
    "release": "npx standard-version"
  },
  "author": "puku0x",
  "license": "MIT",
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0"
  },
  "standard-version": {
    "releaseCommitMessageFormat": "chore(*): bump v{{currentTag}}",
    "skip": {
      "tag": true
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
